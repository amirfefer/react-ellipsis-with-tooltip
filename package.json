{
  "name": "react-ellipsis-with-tooltip",
  "version": "1.0.6",
  "description": "truncates (with ellipsis) overflowing text elements and adds a tooltip",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/amirfefer/react-ellipsis-with-tooltip.git"
  },
  "author": "amirfefer",
  "bugs": {
    "url": "https://github.com/amirfefer/react-ellipsis-with-tooltip/issues"
  },
  "homepage": "https://github.com/amirfefer/react-ellipsis-with-tooltip",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build:clean && npm run build:scripts",
    "build:clean": "rimraf dist",
    "build:scripts": "babel src --out-dir dist/ --ignore *.test.js",
    "test": "jest",
    "lint": "eslint ./src ./stories",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "peerDependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.6.2 || ^16.0.0",
    "react-dom": "^15.6.2 || ^16.0.0",
    "react-bootstrap": "^0.31.0"
  },
  "dependencies": {
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@storybook/react": "^3.3.3",
    "@storybook/storybook-deployer": "^2.2.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^22.0.4",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^4.9.0",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^22.0.4",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.2.0",
    "rimraf": "^2.6.2"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js*"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "modulePaths": [
      "./src"
    ],
    "setupFiles": [
      "raf/polyfill"
    ]
  }
}
